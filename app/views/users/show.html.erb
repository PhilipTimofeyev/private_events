<h1> <%= @user.email %> Events </h1>

<ul>
  <h3> Created Events: </h3>
  <% @user.created_events.each do |event| %>
  <div>
    <p> <%= "Date: #{event.date}" %> </p>
    <p> <%= "Location: #{event.location}"%> </p>
  </div>
  <% end %>
</ul>

<h3> Attended Events: </h3>
<ul>
  <% @user.attended_events.each do |event| %>
  <div>
    <p> <%= "Date: #{event.date}" %> </p>
    <p> <%= "Location: #{event.location}"%> </p>
  </div>
  <% end %>
</ul>